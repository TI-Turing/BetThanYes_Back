{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Start Azure Functions",
      "type": "node-terminal",
      "request": "launch",
      "command": "func start",
      "cwd": "${workspaceFolder}/BetThanYes.Functions"
    },
    {
      "name": "Attach to .NET Functions",
      "type": "coreclr",
      "request": "attach",
      "processId": "${command:azureFunctions.pickProcess}"
    }
  ]
}
